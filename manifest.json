 "manifest_version": 3,
       "name": "Panoptes",
       "version": "0.2",
       "description": "Highlights subversive accounts on X.",
       "permissions": ["activeTab", "storage", "https://dynaboss1337.github.io/Panoptes/*"],
       "host_permissions": ["https://x.com/*", "https://twitter.com/*"],
       "content_scripts": [
         {
           "matches": ["https://x.com/*", "https://twitter.com/*"],
           "js": ["content.js"],
           "css": ["styles.css"]
         }
       ],
       "background": {
         "service_worker": "background.js"
       },
       "icons": {
         "48": "icon48.png",
         "128": "icon128.png"
       }
     }
]
